{
"apiVersion": "cmf.confluent.io/v1alpha1",
"kind": "FlinkApplication",
"metadata": {
   "name": "flink-replication-anonymization-example"
},
"spec": {
   "flinkConfiguration": {
      "metrics.reporter.prom.factory.class": "org.apache.flink.metrics.prometheus.PrometheusReporterFactory",
      "metrics.reporter.prom.port": "9249-9250",
      "taskmanager.numberOfTaskSlots": "1"
   },
   "flinkVersion": "v1_19",
   "image": "us-central1-docker.pkg.dev/solutionsarchitect-01/flink-replication-anonymization/flink-replication-anonymization:0.0.8",
   "job": {
      "jarURI": "local:///opt/flink/Application.jar",
      "parallelism": 3,
      "state": "running",
      "upgradeMode": "stateless"
   },
   "jobManager": {
      "resource": {
      "cpu": 1,
      "memory": "1048m"
      }
   },
   "serviceAccount": "flink",
   "taskManager": {
      "resource": {
      "cpu": 1,
      "memory": "1048m"
      }
   }
}
}
